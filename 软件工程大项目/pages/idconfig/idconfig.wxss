/* pages/idconfig/idconfig.wxss */
.container1 {
  position: absolute;
  top: 0%;
  height: 100%;
  width: 100%;
  background-image: url('https://bucketofpicture.oss-cn-hangzhou.aliyuncs.com/cbb820543001497db8691152b063b5c5.png');
  background-size: 100% 100%;
  background-position: center;
}
.head {
  position: relative;
  top: 0%;
  height: 60px;
  width: 100%;
  border-radius: 20px;
border: 1px solid #FFF;
background: rgba(255, 255, 255, 0.40);
box-shadow: 0px 4px 20px 0px rgba(0, 0, 0, 0.10);
display: flex;
justify-content: center;
align-items: center;
font-family: STFangsong;
font-size: 30px;
font-style: normal;
font-weight: 400;
line-height: normal;
}
.item {
  display: flex;
  position: relative;
  top: 12px;
  left: 2.5%;
  height: 60px;
  width: 95%;
  border-radius: 20px;
border: 1px solid #FFF;
background: rgba(255, 255, 255, 0.40);
box-shadow: 0px 4px 20px 0px rgba(0, 0, 0, 0.10);
margin-bottom: 5px;
}
.content {
  position: relative;
  top: 20%;
  left: 5%;
  color: #000;
font-family: STFangsong;
font-size: 27px;
font-style: normal;
font-weight: 400;
line-height: normal;
}
.moinput {
  position: relative;
  top: 33%;
  left: 50%;
  width: 30%;
  color: #000;
  font-family: STFangsong;
  font-size: 20px;
  font-style: normal;
  font-weight: 400;
  line-height: normal;
}