/* pages/my/my.wxss */
.container {
  position: absolute;
  height: 100%;
  width: 100%;
}
.pingtai {
  position: absolute;
  top: 0%;
  left: 0%;
  width: 375px;
height: 40px;
flex-shrink: 0;
color: #000;
font-family: Inter;
font-size: 28px;
font-style: normal;
font-weight: 400;
line-height: normal;
}
.diver {
  position: absolute;
  top: 7%;
  left: 0%;
  width: 100%;
  height: 1px;
  background-color: #000;
}
.selfinfo {
  position: absolute;
  background: #D9D9D9;
  top: 8%;
  width: 90%;
  height: 20%;
}
.head {
  position: absolute;
  width: 15%;
  height: 40%;
  top: 2%;
  left: 3%;
  border-radius: 50%;
  overflow: hidden;
  border: 2px solid #ccc;
}
.headimg {
  max-width: 100%;
  max-height: 100%;
  border-radius: 50%;
}
.name {
  position: absolute;
  top: 2%;
  left: 21%;
  color: #000;
font-family: Inter;
font-size: 20px;
font-style: normal;
font-weight: 400;
line-height: normal;
width: 106px;
height: 24px;
flex-shrink: 0;
}
.number {
  position: absolute;
  top: 26%;
  left: 21%;
}
.set {
  position: absolute;
  top: 3%;
  right: 3%;
  height: 25px;
  width: 25px;
}
.set image {
  max-width: 100%;
  max-height: 100%;
}
.more {
  position: absolute;
  top: 30%;
  width: 100%;
  height: 60%;
}
.moreitem {
  position: relative;
  height: 18%;
  width: 90%;
  left: 5%;
  margin-bottom: 2%;
  background: #D9D9D9;
}
.info {
  position: absolute;
  left: 2%;
  top: 15%;
  height: 70%;
  width: 40%;
}
.info image {
  position: absolute;
  top: 19%;
  width: 28px;
height: 28px;
flex-shrink: 0;
border-radius: 28px;
}
.info text {
  position: absolute;
  top: 10%;
  left: 25%;
  width: 106px;
height: 29px;
flex-shrink: 0;
color: #000;
font-family: Inter;
font-size: 25px;
font-style: normal;
font-weight: 400;
line-height: normal;
}
.jiantou {
  position: absolute;
  top: 30%;
  right: 2%;
  width: 30px;
height: 30px;
flex-shrink: 0;
border-radius: 28px;
}