.container1 {
  position: absolute;
  top: 0%;
  height: 100%;
  width: 100%;
  background-image: url('https://bucketofpicture.oss-cn-hangzhou.aliyuncs.com/cbb820543001497db8691152b063b5c5.png');
  background-size: 100% 100%;
  background-position: center;
}
.container2 {
  position: absolute;
  top: 0%;
  height: 100%;
  width: 100%;
  background-image: url('https://bucketofpicture.oss-cn-hangzhou.aliyuncs.com/870c6b9d51e14d92a5afe264de71ece1.png');
  background-size: 100% 100%;
  background-position: center;
}
.head {
  position: relative;
  top: 1%;
  height: 8%;
  width: 100%;
  border-radius: 20px;
border: 1px solid #FFF;
background: rgba(255, 255, 255, 0.40);
box-shadow: 0px 4px 20px 0px rgba(0, 0, 0, 0.10);
display: flex;
justify-content: center;
align-items: center;
font-family: STFangsong;
font-size: 30px;
font-style: normal;
font-weight: 400;
line-height: normal;
}
.code {
  position: relative;
  margin-top: 5%;
  border-radius: 20px;
border: 1px solid #FFF;
background: rgba(255, 255, 255, 0.40);
display: flex;
flex-direction: column;
align-items: center;
}
.code text{
  font-family: STFangsong;
font-size: 28px;
font-style: normal;
font-weight: 400;
line-height: normal;
}
.code image{
  margin-top: 10px;
  border-radius: 20px;
  max-width: 98%;
  max-height: 98%;
}
