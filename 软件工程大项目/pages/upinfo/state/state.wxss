/* pages/upinfo/state/state.wxss */
.container1 {
  position: absolute;
  top: 0%;
  height: 100%;
  width: 100%;
  background-image: url('https://bucketofpicture.oss-cn-hangzhou.aliyuncs.com/cbb820543001497db8691152b063b5c5.png');
  background-size: 100% 100%;
  background-position: center;
}
.container2 {
  position: absolute;
  top: 0%;
  height: 100%;
  width: 100%;
  background-image: url('https://bucketofpicture.oss-cn-hangzhou.aliyuncs.com/870c6b9d51e14d92a5afe264de71ece1.png');
  background-size: 100% 100%;
  background-position: center;
}
.out {
  position: absolute;
  top: 5%;
  height: 95%;
  width: 100%;
}
.map {
  position: relative;
  width: 100%;
  height: 360px;
  background-image: url('https://bucketofpicture.oss-cn-hangzhou.aliyuncs.com/b9824438fecc42278d7600b538bf5b7e.png');
  background-size: 100% 100%;
  background-position: center;
}
.state {
  position: relative;
  top: 10px;
  width: 100%;
  background-color: rgb(255, 255, 255,0);
}
.state .box{
  position: relative;
  top: 5px;
  left: 5%;
  width: 90%;
  display: flex;
}
.box .jiantou {
  position: relative;
  top: 15px;
  width: 50px;
  height: 50px;
  border-radius: 50%;
  background-color: rgb(244, 66, 66,0.5)
}
.box .info {
  position: relative;
  top: 0px;
  left: 3%;
  width: 75%;
  flex-shrink: 0;
  border-radius: 10px;
  border: 1px solid #FFF;
  background: rgba(255, 255, 255, 0.4);
  box-shadow: 0px 4px 20px 0px rgba(0, 0, 0, 0.10);
}
.orderstate {
  position: relative;
  color: #000;
  height: 25px;
font-family: Inter;
font-size: 16px;
font-style: normal;
font-weight: 400;
line-height: normal;
}
.time {
  position: absolute;
  right: 5%;
  color: #000;
  font-family: Inter;
  font-size: 13px;
  font-style: normal;
  font-weight: 400;
  line-height: normal;
}
.info view {
  display: flex;
margin-top: 15rpx;
}
.sanjiaoxing {
  position: relative;
  top: 15px;
  width: 20px;
  height: 30px;
  left: 0%;
  border-radius: 10px;
background: rgba(255, 255, 255, 0.40);
mix-blend-mode: soft-light;
box-shadow: 4px 4px 4px 0px rgba(0, 0, 0, 0.50);
}
.sanjiaoxing image {
  max-width: 100%;
  max-height: 100%;
}
.shoot {
  position: relative;
  top: 10rpx;
  background-color: #910961;
  left: 7%;
  width: 1px;
  height: 180rpx;
}
.shoot image {
  max-width: 100%;
  max-height: 100%;
}
.confirm {
  position: relative;
  top: -15rpx;
  left: 70%;
  width: 70px;
  height: 30px;
  border-radius: 10px;
  background: linear-gradient(0deg, rgba(244, 66, 66, 0.20) 0%, rgba(244, 66, 66, 0.20) 100%), #FFB0FC;
display: flex;
justify-content: center;
align-items: center;
color: #FFF;
font-family: Inter;
font-size: 18px;
font-style: normal;
font-weight: 400;
line-height: normal;
}
.huowu {
  position: relative;
  top: 20px;

  left: 15%;
  width: 60px;
  height: 60px;
}
.huowu image {
  border: white 1px;
  border-style: solid;
  border-radius: 10%;
  max-width: 100%;
  max-height: 100%;
}
.finalimg {
  position: relative;
  top: 5px;
  left: 25%;
  width: 60px;
  height: 60px;
}
.finalimg image {
  max-width: 100%;
  max-height: 100%;
}
.zhanwei {
  position: relative;
  top: 4px;
  height: 50px;
  width: 100%;
}
