<!--pages/upinfo/state/state.wxml-->
<view class="container">
  <tabBar selected="0"></tabBar>
  <scroll-view class='out' scroll-y>
    <view class="map"></view>
    <view class="state">
    <block wx:for="{{statu}}" wx:key="unique">
      <view class="box">
        <view class="jiantou">
        </view>
        <view class="info">
          <view><text class="orderstate">{{item.message}}</text>
            <text class="time">{{item.createTime}}</text>
          </view>
          <view><text class="orderstate">{{message[item.type]}}</text>
          </view>
        </view>
      </view>
      <view class="box" wx:if="{{index!=statu.length-1}}">
        <view class="shoot">
        </view>
      </view>
    </block>

    </view>
    <view class="zhanwei"></view>
  </scroll-view>
</view>